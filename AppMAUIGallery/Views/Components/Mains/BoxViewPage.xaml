<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppMAUIGallery.Views.Components.Mains.BoxViewPage"
             Title="BoxViewPage">
    <VerticalStackLayout Padding="10">
        <BoxView Color="Black" HeightRequest="3" ></BoxView>
        <BoxView Color="Blue" HeightRequest="100" WidthRequest="100" ></BoxView>
        <BoxView Color="Green" HeightRequest="200" WidthRequest="100" ></BoxView>
        <BoxView Color="Red" CornerRadius="50" HeightRequest="100" WidthRequest="100" ></BoxView>
    </VerticalStackLayout>
</ContentPage>